{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "{name}",
        "namespace": "{namespace}"
    },
    "spec": {
        "selector": {
            "app": "{name}"
        },
        "ports": [{
            "port": "{port}",
            "targetPort": "{port}"
        }]
    }
}